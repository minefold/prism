<!doctype html>
<html>
<head>
  <title>Proxy Info</title>
</head>
<body>
  <h1>Workers</h1>
  <ul>
    <% WORKERS.each do |worker| %>
    <li>
      <%= worker[:id] %>
      <%= worker[:state] %>
      <ul>
        <% worker[:worlds].each do |world| %>
        <li>
          <%= world[:id] %>
          <%= world[:state] %>
        </li>
        <% end%>
    </li>
    <% end %>
  </ul>
</body>
</html>
